/* roomDetail.wxss */
.header {
	margin-bottom: 20rpx;
	background-color: #FFFFFF;
	padding: 10rpx 30rpx;
	border-bottom:1rpx solid #D9D9D9;
}

.header-icon {
	font-size: 60rpx;
	display: inline-block;
	vertical-align: middle;
}

.header-msg {
	display: inline-block;
	vertical-align: middle;
	margin-left: 40rpx;
}

.header-name {
	font-weight: bold;
}

.header-desc {
	font-size: 26rpx;
	color: #666666;
}

.date-container {
	margin-top: 20rpx;
}

.date-title {
	height: 88rpx;
	line-height: 88rpx;
}